local g=game:GetService("HttpService")local c="https://raw.githubusercontent.com/MommyW6/Main/refs/heads/home/Version"local Q=nil local P=nil local Z=nil local function k()local Q,P=pcall(function()local Q=http_request({Url=c,Method="GET"})return g:JSONDecode(Q.Body)end)return Q and P or nil end local function p()Q=k()if not Q then return end P=Q.Version for g,c in pairs(Q.Games)do(getgenv())[g]=c end end local function J()local g=k()if not g then return end local c=g.Version local Q=g.Games local p={}for g,c in pairs(Q)do if(getgenv())[g]~=c then table.insert(p,g);(getgenv())[g]=c end end local J=c~=P local a=""if J and#p>0 then a="There was an update for ("..(table.concat(p,", ")..(") and the Version ("..(P..(" -> "..(c.."). Would you like to reload?")))))elseif J then a="A minor Update was just made. Would you like to reload? (Version: "..(P..(" -> "..(c..")")))elseif#p>0 then a="There was an update for ("..(table.concat(p,", ")..(") However the Version stays ("..(P.."). Would you like to reload?")))end if a~=""and a~=Z then Window:Dialog({Title=Window.Settings.Title;Description=a;Buttons={{Name="Confirm";Callback=function()task.spawn(function()pcall(function()(game.CoreGui:FindFirstChild("OpenUI")):Destroy()end)pcall(function()(game.CoreGui:FindFirstChild("Mommyware")):Destroy()end);(getgenv()).DisableElements()task.wait(1.5);(loadstring(game:HttpGet("https://raw.githubusercontent.com/MommyW6/Main/home/Loader")))()end)end};{Name="Cancel"}}})Z=a end P=c end local function a()p()while true do J()task.wait(25)end end if not(getgenv()).UpdateNotificationShown==true then(getgenv()).UpdateNotificationShown=true a()end
